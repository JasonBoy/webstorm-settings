<templateSet group="React">
  <template name="st" value="this.state$END$" description="Access component's state" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_EXPRESSION" value="true" />
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="imsc" value="import styled from 'styled-components';" description="import styled from 'styled-components'" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="sss" value="this.setState(state =&gt; {&#10;  $END$&#10;});" description="this.setState(state=&gt;{})" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="sst" value="this.setState({$END$});" description="Performs a shallow merge of nextState into current state" toReformat="true" toShortenFQNames="true">
    <context />
  </template>
  <template name="dstate" value="const {$END$} = this.state;" description="Destructuring this.state" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="dprops" value="const {$END$} = this.props;" description="Destructuring props" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="impts" value="import PropTypes from 'prop-types';" description="import prop-types" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="sd" value="const Styled$CompontName$ = styled.$TAG$`&#10;  $END$&#10;`;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="CompontName" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="TAG" expression="" defaultValue="&quot;div&quot;" alwaysStopAt="true" />
    <context>
      <option name="ECMAScript6" value="true" />
      <option name="JAVA_SCRIPT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="ushook" value="const [$STATE$, $SET_STATE$] = useState($INITIAL_STATE$);" description="React useState() hook" toReformat="false" toShortenFQNames="true">
    <variable name="STATE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="SET_STATE" expression="concat(&quot;set&quot;, capitalize(STATE))" defaultValue="" alwaysStopAt="false" />
    <variable name="INITIAL_STATE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="ECMAScript6" value="true" />
    </context>
  </template>
</templateSet>